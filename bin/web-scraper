#!/usr/bin/env python3
import argparse

from web_scraper import Runner, WebSources


def main():
    parser = argparse.ArgumentParser(description='Extract data from some websites for testing.')
    parser.add_argument('source', type=WebSources, choices=WebSources,
                        help='Source from where to extract the data.')
    args = parser.parse_args()

    runner = Runner()
    runner.run(args)


if __name__ == '__main__':
    main()
